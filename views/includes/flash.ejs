


    <% if(success && success.length){ %>
        <div class="alert alert-primary alert-dismissible fade show" role="alert" style="margin-left: 8rem; margin-right: 8rem" role="alert">
           <%= success %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
    <% } %>
    

<% if(error && error.length){ %>
    <div class="alert alert-primary alert-dismissible fade show" role="alert" style="margin-left: 8rem; margin-right: 8rem" role="alert">
       <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
<% } %>

